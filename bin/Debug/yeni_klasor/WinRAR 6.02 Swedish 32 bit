<script type="text/javascript">
$(document).ready(function() {
$('#tx_indexedsearch[sword]').on('keypress', function(e){
      if(e.which == 13){
        $('#search').submit();
       }
       return false;
      });

$('#searchfield').on('click', function() {
  $('#searchfield').attr('value','');
  return false;
});

var browserName = "";
var parser = new UAParser();
var result = parser.getResult();
console.log(result.browser.name);
console.log(result.browser.version);
browserName = result.browser.name;
document.getElementById("helper_download").style.display = "none";
document.getElementById("Chrome").style.display = "none";
document.getElementById("Firefox").style.display = "none";
document.getElementById("Edge").style.display = "none";
document.getElementById("IE").style.display = "none";

function posHelper(bn)
{
  var showHelp = ""
    switch (bn)
    {
        case "Firefox":
            showHelp = "top-right";
            break;
        case "Chrome":
            showHelp = "bottom-left";
            break;
        case "Edge":
            showHelp = "bottom-center";
            break;
        case "IE":
            showHelp = "bottom-center";
            break;
        default:
            showHelp = "";
    }
    return showHelp;
}
 
function displayHelper()
{
    var getPos = posHelper(browserName);
    if(getPos == "")
    {
        document.getElementById("helper_download").style.display = "none";
        document.getElementById("helper").style.display = "none";
    }
    else
    {
        $('body').addClass('no-scroll');
        $('#helper_download').addClass(getPos);
        $('#helper_download').addClass(browserName);
        document.getElementById("helper_download").style.display = "block";
        if(browserName == "Chrome" || browserName == "Edge" || browserName == "IE")
        {
            document.getElementById(browserName).style.display = "flex";
        }
        else
        {
            document.getElementById(browserName).style.display = "block";
        }
        document.getElementById("helper_download").style.display = "block";
        document.getElementById("helper").style.display = "block";
    } 
}

$('a[href^="https://www.win-rar.com/fileadmin/winrar-versions/"]').each(
	function() { 
		$(this).on('click',
			function() { 
				ga('send', 'event', 'Downloads', 'directdownload', $(this).attr("href"));
			}
		)
	}
);

$('tr.wfqbe_even > td > h2 > a').each(
    function() {
		$(this).on('click',
			function() { 
                displayHelper();
            }
        )
}); 

$('tr.wfqbe_odd > td > h2 > a').each(
    function() {
		$(this).on('click',
			function() { 
                displayHelper();
            }
        )
});

$('div.userLink > a').each(
    function() {
	$(this).on('click',
		function() {
		    ga('send', 'event', 'Downloads', 'topdownload', $(this).attr("href")); 
                    displayHelper();
                }
        )
});


});
</script>


